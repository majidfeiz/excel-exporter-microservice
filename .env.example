# ArvanCloud S3-compatible storage credentials
ARVAN_ACCESS_KEY=your-access-key
ARVAN_SECRET_KEY=your-secret-key

# Arvan S3 endpoint (default for Tehran region)
ARVAN_ENDPOINT=https://s3.ir-thr-at1.arvanstorage.ir

# Bucket name and public base URL (e.g., CDN link or Arvan public access URL)
ARVAN_BUCKET=your-bucket-name
ARVAN_PUBLIC_URL=https://your-cdn-domain.ir-thr-at1.arvanstorage.ir/your-bucket-name